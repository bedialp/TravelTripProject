
@model List<TravelTripProject.Models.Siniflar.Hakkimizda>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_TestLayout.cshtml";
}
<div class="content">
    <div class="wrap">
        <div class="about">
            <h3>Hakkimizda</h3>
            @foreach (var item in Model)
            {
                <img src="@item.FotoUrl" alt="" />
                <p>@item.Aciklama </p>
            }
        </div>
    </div>
</div>
